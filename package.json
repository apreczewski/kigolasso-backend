{
  "name": "kigolasso-backend",
  "version": "1.0.0",
  "description": "Backend API para plataforma Kigolasso - gestÃ£o de peladas amadoras",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "tsx src/prisma/seed.ts"
  },
  "keywords": ["football", "soccer", "sports", "booking", "api", "nodejs", "typescript"],
  "author": "apreczewski",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "express": "^4.19.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.4.5",
    "zod": "^3.23.8",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "stripe": "^16.12.0",
    "firebase-admin": "^12.5.0",
    "winston": "^3.14.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/jest": "^29.5.13",
    "@types/supertest": "^6.0.2",
    "typescript": "^5.6.2",
    "tsx": "^4.19.1",
    "eslint": "^8.57.1",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "prettier": "^3.3.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "supertest": "^7.0.0",
    "prisma": "^5.19.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}